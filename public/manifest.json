{
  "manifest_version": 2,
  "name": "COVID-19 Footprint | Coronavirus Live Tracker",
  "author": "Abir Bhushan",
  "description": "COVID-19 Footprint provides real-time global and regional data about the novel Coronavirus outbreak.",
  "version": "1.0.1",
  "icons": {
    "128": "icons/128.png",
    "48": "icons/48.png",
    "16": "icons/16.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": {
      "128": "icons/128.png",
      "48": "icons/48.png",
      "16": "icons/16.png"
    },
    "default_title": "COVID-19 Footprint | Coronavirus Live Tracker",
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "content_security_policy": "script-src 'self' 'sha256-uevKEqHqDF6X1aFytcl7R7FsPmOFPun/Y4drRKXDNbw='; object-src 'self'"
}
